    <DatevBookingStackFieldDefinition fieldId="96" fieldName="Buchungstyp" type="string" maxLength="2">
    </DatevBookingStackFieldDefinition>
    # AA = Angeforderte Anzahlung/Abschlagsrechnung
    # AG = Erhaltene Anzahlung (Geldeingang)
